{
    "name": "InstaChat",
    "description": "An AI based Instagram Chatbot project",
    "repository": "https://github.com/ChankitSaini/InstaChat",
    "env": {
        "username": {
            "description": "Instagram Username of chatbot account (without @)",
            "required": true
        },
        "password": {
            "description": "Instagram Password Here",
            "required": true
        },
        "BotName": {
            "description": "Instagram BotName",
            "required": true
        },
        "OwnerName": {
            "description": "Owner Name",
            "required": true
        }
    },
    "addons": [
        {
            "plan": "heroku-postgresql"
        }
    ]
}
